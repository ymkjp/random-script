{
    "name": "random-script",
    "description": "Random Unicode string generator library",
    "version": "0.1.0",
    "repository": {
        "type": "git",
        "url": "https://github.com/ymkjp/random-script"
    },
    "license": "MIT",
    "main": "./lib/main.js",
    "files": [
        "lib",
        "data"
    ],
    "directories": {
        "test": "test"
    },
    "dependencies": {
        "random-js": "~1.0.8"
    },
    "devDependencies": {
        "babel-cli": "^6.5.1",
        "babel-core": "^6.5.1",
        "babel-plugin-add-module-exports": "^0.1.2",
        "babel-plugin-transform-es2015-modules-commonjs": "^6.5.0",
        "babel-preset-es2015": "^6.5.0",
        "espower-babel": "^4.0.1",
        "mocha": "^2.4.5",
        "power-assert": "^1.2.0",
        "unicode-data": "mathiasbynens/node-unicode-data"
    },
    "scripts": {
        "generate": "node script/generate-*.js",
        "compile": "babel --out-dir lib/ src/",
        "prepublish": "npm run compile",
        "pretest": "npm run compile",
        "test": "mocha --compilers js:espower-babel/guess"
    },
    "babel": {
        "presets": [
            "es2015"
        ],
        "plugins": [
            "add-module-exports"
        ]
    }
}
